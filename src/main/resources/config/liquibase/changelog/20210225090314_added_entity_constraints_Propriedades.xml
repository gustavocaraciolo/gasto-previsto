<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Propriedades.
    -->
    <changeSet id="20210225090314-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="propriedades"
                                 constraintName="fk_propriedades_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="propriedades_id"
                                 baseTableName="propriedades"
                                 constraintName="fk_propriedades_propriedades_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tipo_financeiro"/>

        <addForeignKeyConstraint baseColumnNames="propriedades_id"
                                 baseTableName="propriedades_anexo"
                                 constraintName="fk_propriedades_anexo_propriedades_id"
                                 referencedColumnNames="id"
                                 referencedTableName="propriedades"/>

        <addForeignKeyConstraint baseColumnNames="anexo_id"
                                 baseTableName="propriedades_anexo"
                                 constraintName="fk_propriedades_anexo_anexo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="anexo"/>
        
    </changeSet>
</databaseChangeLog>
